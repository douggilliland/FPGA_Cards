LIBRARY IEEE;
USE IEEE.STD_LOGIC_1164.ALL;
USE IEEE.STD_LOGIC_UNSIGNED.ALL

ENTITY JOYSTICK IS
PORT(
		CLK: IN STD_LOGIC,
		RESET: IN STD_LOGIC,
		DATA: IN STD_LOGIC_VECTOR(3 DOWNTO 0),
		LED: OUT STD_LOGIC_VECTOR(3 DOWNTO 0)
     );
	  

END JOYSTICK

ARCHITECTURE LED OF JOYSTICK IS
BEGIN
LED<=DATA;
END

