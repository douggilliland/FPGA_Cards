FUNCTION sign_div_unsign_rlh (denominator[3..0], numerator[16..0])
RETURNS ( quotient[16..0], remainder[3..0]);

--synthesis_resources = lut 87 
SUBDESIGN lpm_divide_6bm
( 
	denom[3..0]	:	input;
	numer[16..0]	:	input;
	quotient[16..0]	:	output;
	remain[3..0]	:	output;
) 
VARIABLE 
	divider : sign_div_unsign_rlh;
	numer_tmp[16..0]	: WIRE;

BEGIN 
	divider.denominator[] = denom[];
	divider.numerator[] = numer_tmp[];
	numer_tmp[] = numer[];
	quotient[] = divider.quotient[];
	remain[] = divider.remainder[];
END;
