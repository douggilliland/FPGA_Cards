module key_b
	(
		Key,
		Led
	);
	input [2:0] Key;
	output [2:0] Led;
	
	reg [2:0] Led;
	
	always @ (Key,Led)
		case(Key)
			3'b000:Led=3'b000;
			3'b001:Led=3'b001;
			3'b010:Led=3'b010;
			3'b011:Led=3'b011;
			3'b100:Led=3'b100;
			3'b101:Led=3'b101;
			3'b110:Led=3'b110;
			3'b111:Led=3'b111;
			default:Led=3'b000;
		endcase  
endmodule