FUNCTION altsyncram_cqa1 (aclr0, address_a[0..0], address_b[0..0], clock0, clocken0, data_a[7..0], wren_a)
RETURNS ( q_b[7..0]);
FUNCTION cntr_ikf (clk_en, clock)
RETURNS ( q[0..0]);
FUNCTION cntr_94h (aset, clk_en, clock, cnt_en)
RETURNS ( cout, q[1..0]);

--synthesis_resources = lut 3 M4K 1 reg 4 
SUBDESIGN shift_taps_rvm
( 
	aclr	:	input;
	clken	:	input;
	clock	:	input;
	shiftin[7..0]	:	input;
	shiftout[7..0]	:	output;
	taps[7..0]	:	output;
) 
VARIABLE 
	altsyncram2 : altsyncram_cqa1;
	dffe4 : dffe;
	cntr1 : cntr_ikf;
	cntr3 : cntr_94h;

BEGIN 
	altsyncram2.aclr0 = dffe4.q;
	altsyncram2.address_a[] = cntr1.q[];
	altsyncram2.address_b[] = cntr1.q[];
	altsyncram2.clock0 = clock;
	altsyncram2.clocken0 = clken;
	altsyncram2.data_a[] = ( shiftin[]);
	altsyncram2.wren_a = B"1";
	dffe4.clk = clock;
	dffe4.d = (! cntr3.cout);
	dffe4.ena = clken;
	dffe4.prn = (! aclr);
	cntr1.clk_en = clken;
	cntr1.clock = clock;
	cntr3.aset = aclr;
	cntr3.clk_en = clken;
	cntr3.clock = clock;
	cntr3.cnt_en = (! cntr3.cout);
	shiftout[7..0] = altsyncram2.q_b[7..0];
	taps[] = altsyncram2.q_b[];
END;
